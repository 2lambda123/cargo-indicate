
FullQuery(
    query: r#"
{
    RootPackage {
        dependencies {
            advisoryHistory(includeWithdrawn: $include_withdrawn) {
                affectedFunctions {
                    functionPath @output
                    versions @output
                }
            }
        }
    }
}
    "#,
    args: {
        "include_withdrawn": Boolean(true),
    }
)